{
  "name": "eslint-plugin-jsonschema",
  "version": "0.2.0",
  "description": "eslint plugin for jsonschema",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "okitakunio@gmail.com",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -r lib || true && babel src -d lib",
    "lint": "eslint src",
    "test": "npm run-script build && jest tests/** --verbose && mocha tests --require babel-register --recursive",
    "test:watch": "SOURCE_DIR=src mocha tests --watch --recursive --require babel-register tests"
  },
  "dependencies": {
    "ajv": "^4.11.8",
    "esprima": "^4.0.1",
    "jsonpointer": "^4.0.1",
    "requireindex": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/register": "^7.4.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-plugin-espower": "^2.4.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "~3.9.1",
    "mocha": "^3.2.0"
  },
  "engines": {
    "node": ">=5"
  },
  "license": "ISC"
}
