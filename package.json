{
  "name": "eslint-plugin-jsonschema",
  "version": "0.3.0",
  "description": "eslint plugin for jsonschema",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "okitakunio@gmail.com",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -r lib || true && babel src -d lib",
    "lint": "eslint src",
    "test": "npm run-script build && jest tests/** --verbose",
    "test:watch": "SOURCE_DIR=src mocha tests --watch --recursive --require babel-register tests"
  },
  "dependencies": {
    "ajv": "^4.11.8",
    "jsonpointer": "^4.0.1",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/register": "^7.4.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-espower": "^3.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "eslint": "~5.15.3",
    "esprima": "^4.0.1",
    "jest": "^24.5.0"
  },
  "engines": {
    "node": ">=5"
  },
  "license": "ISC"
}
