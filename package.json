{
  "name": "eslint-plugin-jsonschema",
  "version": "0.0.0",
  "description": "eslint plugin for jsonschema",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "okitakunio@gmail.com",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -r lib || true && babel src -d lib",
    "lint": "eslint src",
    "test":       "mocha tests --require babel-register --recursive",
    "test:watch": "mocha tests --watch --require babel-register tests",
    "watch": "babel --watch src -d lib"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-espower": "^2.3.2",
    "babel-preset-env": "^1.2.1",
    "babel-register": "^6.24.0",
    "eslint": "^3.9.1",
    "generator-eslint": "^1.2.0",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2",
    "watch-run": "^1.2.5",
    "yo": "^1.8.5"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "license": "ISC"
}
